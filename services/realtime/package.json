{
    "name": "clowdr-presence",
    "version": "1.0.0",
    "description": "Presence counters",
    "main": "build/web-server.js",
    "engines": {
        "node": "16.x",
        "npm": "^7.8.x"
    },
    "scripts": {
        "build": "npm run build-shared && npm run build-this && npm run-script copy-resources",
        "build-this": "ttsc",
        "build-shared": "npm run --prefix=../../ --workspace=shared build",
        "copy-resources": "rimraf build/resources && cpy --cwd=src/resources \"**/*\" ../../build/resources --parents",
        "start": "node build/server.js",
        "dev-stage2": "tsc-watch --compiler \"ttypescript/bin/tsc\" --onSuccess \"npm run-script --prefix=../../ run-services-realtime\"",
        "dev": "npm run build-shared && node-env-run -E ./.env -e \"npm run dev-stage2\"",
        "gql-cdg": "npx --no-install -p @graphql-codegen/cli graphql-codegen -- --config codegen.js",
        "test-publisher": "node-env-run -E ./src/test/.publisher.env -e \"ts-node ./src/test/publisher.ts\"",
        "test-consumer": "node-env-run -E ./src/test/.consumer.env -e \"ts-node ./src/test/consumer.ts\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/clowdr/clowdr-app.git"
    },
    "author": "Clowdr CIC",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/clowdr/clowdr-app/issues"
    },
    "homepage": "https://github.com/clowdr/clowdr-app#readme",
    "dependencies": {
        "@apollo/client": "^3.3.20",
        "@graphql-codegen/cli": "^1.21.6",
        "@graphql-codegen/fragment-matcher": "^2.0.1",
        "@graphql-codegen/introspection": "^1.18.2",
        "@graphql-codegen/typed-document-node": "^1.18.9",
        "@graphql-codegen/typescript": "^1.22.4",
        "@graphql-codegen/typescript-document-nodes": "^1.17.15",
        "@graphql-codegen/typescript-graphql-files-modules": "^1.18.1",
        "@graphql-codegen/typescript-graphql-request": "^3.2.4",
        "@graphql-codegen/typescript-operations": "^1.18.3",
        "@socket.io/redis-emitter": "^4.1.0",
        "@types/amqplib": "^0.8.0",
        "@types/express": "^4.17.12",
        "@types/jsonwebtoken": "^8.5.3",
        "@types/jwk-to-pem": "^2.0.0",
        "@types/node-fetch": "^2.5.10",
        "@types/redis": "^2.8.30",
        "@types/redlock": "^4.0.1",
        "@types/socket.io": "^3.0.1",
        "@types/socket.io-redis": "^1.0.27",
        "@types/uuid": "^8.3.0",
        "@types/web-push": "^3.3.1",
        "@types/ws": "^7.4.5",
        "amqplib": "^0.8.0",
        "assert": "^2.0.0",
        "bufferutil": "^4.0.3",
        "cpy-cli": "^3.1.1",
        "express": "^4.17.1",
        "generic-pool": "^3.8.2",
        "jsonwebtoken": "^8.5.1",
        "jwk-to-pem": "^2.0.5",
        "jwks-rsa": "^2.0.3",
        "node-env-run": "^4.0.2",
        "node-fetch": "^2.6.1",
        "redis": "^3.1.2",
        "redlock": "^4.2.0",
        "rimraf": "^3.0.2",
        "socket.io": "^3.1.2",
        "socket.io-client": "^4.1.2",
        "socket.io-redis": "^6.0.1",
        "subscriptions-transport-ws": "^0.9.19",
        "ttypescript": "^1.5.12",
        "typescript": "^4.3.5",
        "typescript-is": "^0.18.3",
        "utf-8-validate": "^5.0.5",
        "uuid": "^8.3.2",
        "web-push": "^3.4.5"
    },
    "devDependencies": {
        "tsc-watch": "^4.4.0"
    }
}
